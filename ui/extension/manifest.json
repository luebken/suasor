{

  "description": "Searches for similar repositories.",
  "manifest_version": 2,
  "name": "Suasor",
  "version": "1.0",
  "homepage_url": "http://github.com/luebken/suasor",
  "icons": {
    "48": "icons/stars-48.png"
  },

  "permissions": [
    "*://bluemix.net/*",
    "webRequest",
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/stars-32.png",
    "theme_icons": [{
        "light": "icons/stars-32-light.png",
        "dark": "icons/stars-32.png",
        "size": 32
    }],
    "default_title": "Suasor",
    "default_popup": "popup/choose_beast.html"
  }

}
